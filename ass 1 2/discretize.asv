function y = discretize(x,lower,upper,n)
diff=upper-lower;
width=diff/n;
if(x>=lower && x<lower+width)
    y='F';
    return;
end
if(x>=lower+width && x<lower+2*width)
    y='D';
    return;
end
if(x>= lower+2*width && x<lower+3*width)
    y='C';
    return;
end
if(x>=lower+3*width && x<lower+4*width)
    y='B';
    return;
end
if(x>=lower+4* && x<lower+width)
    y='A';
    return
end
end
